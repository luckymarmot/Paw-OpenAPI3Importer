{
  "name": "openapi-v3-importer",
  "productName": "OpenAPIv3Importer",
  "version": "0.1.0-beta",
  "description": "A Paw Extension to import OpenAPI 3.x.x files into Paw",
  "main": "src/index.ts",
  "license": "MIT",
  "repository": "git@github.com:luckymarmot/Paw-OpenAPI3Importer.git",
  "scripts": {
    "start": "yarn dev:mac",
    "dev:mac": "bash -c \"./node_modules/.bin/webpack --node-env development --watch -o $HOME/$npm_package_config_outputDir/$npm_package_config_identifier\"",
    "dev:mac:static": "bash -c \"./node_modules/.bin/webpack --node-env development -o $HOME/$npm_package_config_outputDir/$npm_package_config_identifier\"",
    "dev:web": "echo \"\n\tWork in progress...\n\"",
    "build:mac": "webpack --config ./webpack.config.babel.js",
    "build:web": "yarn ",
    "test": "jest"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "promise": "^8.1.0",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-classes": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.7.0",
    "@babel/register": "^7.7.0",
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.5",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.6",
    "core-js": "3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^24.9.0",
    "openapi-types": "^7.2.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^4.6.0"
  },
  "config": {
    "name": "OpenAPIv3xImporter-beta",
    "title": "OpenAPI v3.x Importer (beta)",
    "description": "An OpenAPI importer extension for Paw client.",
    "identifier": "com.luckymarmot.PawExtensions.OpenAPIv3xImporter",
    "fileExtensions": [
      "json",
      "yaml",
      "yml"
    ],
    "inputs": "json",
    "help": "https://github.com/luckymarmot/Paw-OpenAPI3Importer/wiki",
    "outputDir": "\"/Library/Containers/com.luckymarmot.Paw/Data/Library/Application Support/com.luckymarmot.Paw/Extensions\""
  }
}
